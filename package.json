{
  "name": "eyecon-bootstrap-slider",
  "id": "github:distros/eyecon-bootstrap-slider",
  "version": "1.0.0",
  "description": "Repository for the bootstrap-slider from Stefan Petre (eyecon.ro)",
  "main": "src/bootstrap-slider.js",
  "registry": "jspm",
  "repository": {
    "type": "git",
    "url": "https://github.com/distros/eyecon-bootstrap-slider.git"
  },
  "homepage": "https://github.com/distros/eyecon-bootstrap-slider",
  "author": "Stefan Petre",
  "maintainers": [
    {
      "name": "Aaike Van Roekeghem",
      "email": "aaikevr@gmail.com"
    }
  ],
  "demos": [
    "http://www.eyecon.ro/bootstrap-slider"
  ],
  "format": "global",
  "shim": {
    "src/bootstrap-slider": [
      "jquery",
      "bootstrap"
    ],
    "dist/bootstrap-slider.min": [
      "jquery",
      "bootstrap"
    ]
  },
  "files": [
    "dist/*",
    "src/*"
  ],
  "directories": {
    "lib": "src",
    "build": "dist"
  },
  "jspm": {
    "directories": {
      "lib": "."
    },
    "dependencies": {
      "jquery": "github:components/jquery",
      "bootstrap": "github:twbs/bootstrap"
    }
  },
  "scripts": {
    "test": "npm run jshint",
    "jshint": "jshint --reporter gulpfile.js src task > badge-jshint.svg"
  },
  "devDependencies": {
    "bootstrap-less": "^3.3.5",
    "deep-extend": "^0.3.2",
    "gulp": "^3.8.11",
    "gulp-clean": "^0.3.1",
    "gulp-less": "^3.0.1",
    "gulp-noop": "git://github.com/gooy/gulp-noop#0.0.1",
    "gulp-rename": "^1.2.0",
    "gulp-run-sequence": "^0.3.2",
    "gulp-uglify": "^1.1.0",
    "gulp-util": "^3.0.4",
    "jshint-reporter-badge": "^0.1.0",
    "less-plugin-autoprefix": "^1.3.0",
    "less-plugin-clean-css": "^1.5.0",
    "requiredir": "^1.0.6",
    "shell-promise": "git://github.com/gooy/shell-promise"
  }
}
